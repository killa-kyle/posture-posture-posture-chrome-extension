{
  "manifest_version": 2,
  "name": "Posture!Posture!Posture!",
  "description": "Sit up Straight! This extension will help you track your posture as you browse with the help of your webcam.",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"]
      
    }
  ],
  "web_accessible_resources":  ["content.styles.css", "icon-128.png", "icon-34.png"]
}
